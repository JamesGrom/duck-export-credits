{
	"manifest_version": 2,
	"name": "Request Logger",
	"version": "1.0",
	"devtools_page": "devtools.html",
	"permissions": [
		"devtools",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"downloads",
		"fileSystem"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	}
}

{
	"manifest_version": 2,
	"name": "Network Response Logger",
	"version": "1.0",
	"description": "Logs the response body of network requests to the console.",
	"devtools_page": "devtools.html",
	"permissions": ["webRequest", "webRequestBlocking", "http://*/*", "https://*/*"],
	"background": {
		"scripts": ["background.js", "devtools.js", "panel.js"],
		"persistent": true
	}
}
